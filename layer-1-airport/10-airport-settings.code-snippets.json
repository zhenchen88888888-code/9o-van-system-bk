',\n            '\u5609\u7fa9\u7e23' => '\u5609\u7fa9\u5730\u5340'\n        ];\n        \n        if (isset($city_mapping[$city])) {\n            $city = $city_mapping[$city];\n        }\n        \n        return isset($prices[$city]) ? $prices[$city] : 3000; \/\/ \u9810\u8a2d\u50f9\u683c\n    }\n    \n    \/**\n     * \u8a08\u7b97\u591c\u9593\u52a0\u50f9\n     *\/\n    public static function calculate_night_surcharge($time) {\n        if (!self::NIGHT_SURCHARGE['enabled']) {\n            return 0;\n        }\n        \n        $hour = intval(date('H', strtotime($time)));\n        $start_hour = intval(date('H', strtotime(self::NIGHT_SURCHARGE['start_time'])));\n        $end_hour = intval(date('H', strtotime(self::NIGHT_SURCHARGE['end_time'])));\n        \n        \/\/ \u8655\u7406\u8de8\u591c\u60c5\u6cc1\n        if ($start_hour > $end_hour) {\n            \/\/ \u4f8b\u5982 22:00 \u5230 08:00\n            if ($hour >= $start_hour || $hour < $end_hour) {\n                return self::NIGHT_SURCHARGE['amount'];\n            }\n        } else {\n            \/\/ \u4e00\u822c\u60c5\u6cc1\n            if ($hour >= $start_hour && $hour < $end_hour) {\n                return self::NIGHT_SURCHARGE['amount'];\n            }\n        }\n        \n        return 0;\n    }\n    \n    \/**\n     * \u8a08\u7b97\u505c\u9760\u9ede\u8cbb\u7528\n     *\/\n    public static function calculate_stopover_charge($distance_km) {\n        foreach (self::STOPOVER_CHARGES as $charge) {\n            if ($distance_km >= $charge['min'] && $distance_km < $charge['max']) {\n                return $charge['price'];\n            }\n            \n            \/\/ \u8655\u7406\u516c\u5f0f\u8a08\u7b97\n            if (isset($charge['price_formula']) && $distance_km >= $charge['min']) {\n                return eval('return ' . str_replace('distance', $distance_km, $charge['price_formula']) . ';');\n            }\n        }\n        \n        return 0;\n    }\n    \n    \/**\n     * \u6aa2\u67e5\u504f\u9060\u5730\u5340\u52a0\u50f9\n     *\/\n    public static function check_remote_area_surcharge($lat, $lng) {\n        $point = ['lat' => $lat, 'lng' => $lng];\n        $remote_areas = self::get_remote_areas();\n        \n        foreach ($remote_areas as $area) {\n            foreach ($area['polygons'] as $polygon) {\n                if (!empty($polygon) && self::is_point_in_polygon($point, $polygon)) {\n                    return [\n                        'surcharge' => $area['surcharge'],\n                        'area_name' => $area['name']\n                    ];\n                }\n            }\n        }\n        \n        return ['surcharge' => 0, 'area_name' => null];\n    }\n    \n    \/**\n     * \u9ede\u5728\u591a\u908a\u5f62\u5167\u6f14\u7b97\u6cd5\n     *\/\n    private static function is_point_in_polygon($point, $polygon) {\n        $x = $point['lng'];\n        $y = $point['lat'];\n        $inside = false;\n        \n        $n = count($polygon);\n        for ($i = 0, $j = $n - 1; $i < $n; $j = $i++) {\n            $xi = $polygon[$i]['lng'];\n            $yi = $polygon[$i]['lat'];\n            $xj = $polygon[$j]['lng'];\n            $yj = $polygon[$j]['lat'];\n            \n            $intersect = (($yi > $y) != ($yj > $y))\n                && ($x < ($xj - $xi) * ($y - $yi) \/ ($yj - $yi) + $xi);\n            \n            if ($intersect) {\n                $inside = !$inside;\n            }\n        }\n        \n        return $inside;\n    }\n    \n    \/**\n     * \u8a08\u7b97\u52a0\u8cfc\u9805\u76ee\u8cbb\u7528\n     *\/\n    public static function calculate_addon_charges($addons) {\n        $total = 0;\n        \n        foreach ($addons as $addon => $quantity) {\n            if (isset(self::ADDON_PRICES[$addon])) {\n                $total += self::ADDON_PRICES[$addon] * $quantity;\n            }\n        }\n        \n        return $total;\n    }\n    \n    \/**\n     * \u5957\u7528\u4f86\u56de\u7a0b\u6298\u6263\n     *\/\n    public static function apply_roundtrip_discount($total) {\n        return round($total * self::ROUNDTRIP_DISCOUNT);\n    }\n    \n    \/**\n     * \u53d6\u5f97\u6240\u6709\u504f\u9060\u5730\u5340\u5217\u8868\n     *\/\n    public static function get_remote_areas_list() {\n        $remote_areas = self::get_remote_areas();\n        $areas = [];\n        \n        foreach ($remote_areas as $area) {\n            $areas[] = [\n                'name' => $area['name'],\n                'surcharge' => $area['surcharge'],\n                'polygon_count' => count($area['polygons'])\n            ];\n        }\n        \n        return $areas;\n    }\n    \n    \/**\n     * \u53d6\u5f97\u8a2d\u5b9a\u6458\u8981\uff08\u4f9b\u5f8c\u53f0\u986f\u793a\uff09\n     *\/\n    public static function get_settings_summary() {\n        $remote_areas = self::get_remote_areas();\n        $total_polygons = 0;\n        \n        foreach ($remote_areas as $area) {\n            $total_polygons += count($area['polygons']);\n        }\n        \n        return [\n            'tpe_cities' => count(self::TPE_BASE_PRICES),\n            'tsa_cities' => count(self::TSA_BASE_PRICES),\n            'night_surcharge' => self::NIGHT_SURCHARGE,\n            'remote_areas' => count($remote_areas),\n            'total_polygons' => $total_polygons,\n            'addon_prices' => self::ADDON_PRICES,\n            'roundtrip_discount' => (1 - self::ROUNDTRIP_DISCOUNT) * 100 . '%'\n        ];\n    }\n}\n\n\/\/ ========================================\n\/\/ \u521d\u59cb\u5316\u8a2d\u5b9a\n\/\/ ========================================\nadd_action('init', function() {\n    \/\/ \u8a3b\u518a\u8a2d\u5b9a\u5230 WordPress\n    if (!get_option('9o_van_airport_settings_version')) {\n        update_option('9o_van_airport_settings_version', '1.0.0');\n        update_option('9o_van_airport_settings_updated', current_time('mysql'));\n    }\n});\n\n\/\/ ========================================\n\/\/ \u63d0\u4f9b\u5168\u57df\u51fd\u6578\uff08\u4f9b\u5176\u4ed6\u6a21\u7d44\u4f7f\u7528\uff09\n\/\/ ========================================\n\n\/\/ \u53d6\u5f97\u6a5f\u5834\u50f9\u683c\nif (!function_exists('_9o_van_get_airport_price')) {\n    function _9o_van_get_airport_price($airport, $city) {\n        return \\NineOVan\\Airport\\AirportSettings::get_base_price($airport, $city);\n    }\n}\n\n\/\/ \u6aa2\u67e5\u504f\u9060\u5730\u5340\nif (!function_exists('_9o_van_check_remote_area')) {\n    function _9o_van_check_remote_area($lat, $lng) {\n        return \\NineOVan\\Airport\\AirportSettings::check_remote_area_surcharge($lat, $lng);\n    }\n}\n\n\/\/ \u8a08\u7b97\u591c\u9593\u52a0\u50f9\nif (!function_exists('_9o_van_calculate_night_surcharge')) {\n    function _9o_van_calculate_night_surcharge($time) {\n        return \\NineOVan\\Airport\\AirportSettings::calculate_night_surcharge($time);\n    }\n}\n\n\/\/ \u8a08\u7b97\u505c\u9760\u9ede\u8cbb\u7528\nif (!function_exists('_9o_van_calculate_stopover_charge')) {\n    function _9o_van_calculate_stopover_charge($distance_km) {\n        return \\NineOVan\\Airport\\AirportSettings::calculate_stopover_charge($distance_km);\n    }\n}\n\n\/\/ \u8a08\u7b97\u52a0\u8cfc\u9805\u76ee\nif (!function_exists('_9o_van_calculate_addon_charges')) {\n    function _9o_van_calculate_addon_charges($child_seats, $booster_seats, $name_board) {\n        $addons = [\n            'baby_seat' => $child_seats,\n            'booster_seat' => $booster_seats,\n            'name_board' => ($name_board === 'yes' ? 1 : 0)\n        ];\n        return \\NineOVan\\Airport\\AirportSettings::calculate_addon_charges($addons);\n    }\n}\n\n\/\/ \u5957\u7528\u4f86\u56de\u7a0b\u6298\u6263\nif (!function_exists('_9o_van_apply_roundtrip_discount')) {\n    function _9o_van_apply_roundtrip_discount($total) {\n        return \\NineOVan\\Airport\\AirportSettings::apply_roundtrip_discount($total);\n    }\n}\n\n\/\/ ========================================\n\/\/ \u5f8c\u53f0\u986f\u793a\u8a2d\u5b9a\u8cc7\u8a0a\uff08\u9078\u7528\uff09\n\/\/ ========================================\nadd_action('admin_menu', function() {\n    \/\/ \u6aa2\u67e5\u662f\u5426\u8981\u5728\u5f8c\u53f0\u986f\u793a\u8a2d\u5b9a\n    if (current_user_can('manage_options')) {\n        add_submenu_page(\n            null, \/\/ \u96b1\u85cf\u7684\u5b50\u9078\u55ae\n            '\u6a5f\u5834\u63a5\u9001\u8a2d\u5b9a',\n            '\u6a5f\u5834\u63a5\u9001\u8a2d\u5b9a',\n            'manage_options',\n            '9o-van-airport-settings',\n            function() {\n                $summary = \\NineOVan\\Airport\\AirportSettings::get_settings_summary();\n                ?>\n                <div class=\"wrap\">\n                    <h1>\u6a5f\u5834\u63a5\u9001\u8a2d\u5b9a\u6458\u8981<\/h1>\n                    <table class=\"widefat\">\n                        <tbody>\n                            <tr>\n                                <th>\u6843\u5712\u6a5f\u5834\u670d\u52d9\u57ce\u5e02<\/th>\n                                <td><?php echo $summary['tpe_cities']; ?> \u500b<\/td>\n                            <\/tr>\n                            <tr>\n                                <th>\u677e\u5c71\u6a5f\u5834\u670d\u52d9\u57ce\u5e02<\/th>\n                                <td><?php echo $summary['tsa_cities']; ?> \u500b<\/td>\n                            <\/tr>\n                            <tr>\n                                <th>\u591c\u9593\u52a0\u50f9<\/th>\n                                <td>\n                                    <?php echo $summary['night_surcharge']['enabled'] ? '\u555f\u7528' : '\u505c\u7528'; ?>\n                                    (NT$ <?php echo $summary['night_surcharge']['amount']; ?>)\n                                    <?php echo $summary['night_surcharge']['start_time']; ?> - \n                                    <?php echo $summary['night_surcharge']['end_time']; ?>\n                                <\/td>\n                            <\/tr>\n                            <tr>\n                                <th>\u504f\u9060\u5730\u5340\u6578\u91cf<\/th>\n                                <td><?php echo $summary['remote_areas']; ?> \u500b\u5340\u57df<\/td>\n                            <\/tr>\n                            <tr>\n                                <th>\u4f86\u56de\u7a0b\u6298\u6263<\/th>\n                                <td><?php echo $summary['roundtrip_discount']; ?><\/td>\n                            <\/tr>\n                            <tr>\n                                <th>\u52a0\u8cfc\u9805\u76ee<\/th>\n                                <td>\n                                    \u5b30\u5152\u5ea7\u6905: NT$ <?php echo $summary['addon_prices']['baby_seat']; ?><br>\n                                    \u589e\u9ad8\u588a: NT$ <?php echo $summary['addon_prices']['booster_seat']; ?><br>\n                                    \u8209\u724c\u670d\u52d9: NT$ <?php echo $summary['addon_prices']['name_board']; ?>\n                                <\/td>\n                            <\/tr>\n                        <\/tbody>\n                    <\/table>\n                <\/div>\n                <?php\n            }\n        );\n    }\n});\n\n\/\/ \u8a18\u9304\u6a21\u7d44\u8f09\u5165\nif (defined('NINE_O_VAN_DEBUG') && NINE_O_VAN_DEBUG) {\n    error_log('[10-Airport-Settings] \u6a21\u7d44\u5df2\u8f09\u5165 - v1.0.0');\n}\n","active":true,"modified":"2025-09-15 12:58:57","revision":"1"}]}